#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Asus/Documents/slidev/qqqqqqintenta/node_modules/.pnpm/@slidev+cli@52.6.0_@babel+p_b7d395905672ce1e766851008d3b3229/node_modules/@slidev/cli/bin/node_modules:/mnt/c/Users/Asus/Documents/slidev/qqqqqqintenta/node_modules/.pnpm/@slidev+cli@52.6.0_@babel+p_b7d395905672ce1e766851008d3b3229/node_modules/@slidev/cli/node_modules:/mnt/c/Users/Asus/Documents/slidev/qqqqqqintenta/node_modules/.pnpm/@slidev+cli@52.6.0_@babel+p_b7d395905672ce1e766851008d3b3229/node_modules/@slidev/node_modules:/mnt/c/Users/Asus/Documents/slidev/qqqqqqintenta/node_modules/.pnpm/@slidev+cli@52.6.0_@babel+p_b7d395905672ce1e766851008d3b3229/node_modules:/mnt/c/Users/Asus/Documents/slidev/qqqqqqintenta/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Asus/Documents/slidev/qqqqqqintenta/node_modules/.pnpm/@slidev+cli@52.6.0_@babel+p_b7d395905672ce1e766851008d3b3229/node_modules/@slidev/cli/bin/node_modules:/mnt/c/Users/Asus/Documents/slidev/qqqqqqintenta/node_modules/.pnpm/@slidev+cli@52.6.0_@babel+p_b7d395905672ce1e766851008d3b3229/node_modules/@slidev/cli/node_modules:/mnt/c/Users/Asus/Documents/slidev/qqqqqqintenta/node_modules/.pnpm/@slidev+cli@52.6.0_@babel+p_b7d395905672ce1e766851008d3b3229/node_modules/@slidev/node_modules:/mnt/c/Users/Asus/Documents/slidev/qqqqqqintenta/node_modules/.pnpm/@slidev+cli@52.6.0_@babel+p_b7d395905672ce1e766851008d3b3229/node_modules:/mnt/c/Users/Asus/Documents/slidev/qqqqqqintenta/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/slidev.mjs" "$@"
else
  exec node  "$basedir/../../bin/slidev.mjs" "$@"
fi
